{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "homepage_url": "https://github.com/ConteMan/Contea",
  "description": "View all info in one place",
  "default_locale": "en",
  "permissions": [
    "storage",
    "cookies",
    "tabs",
    "https://flomoapp.com/*",
    "https://web-api.okjike.com/*",
    "https://api.juejin.cn/*",
    "https://www.yuque.com/*",
    "https://www.zhihu.com/*"
  ],
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": {
      "19": "icons/19.png",
      "38": "icons/38.png"
    }
  },
  "options_page": "options.html",
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}
